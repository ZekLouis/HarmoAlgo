import java.awt.BorderLayout;

import javax.swing.JFrame;

public class UI extends JFrame{
	
	public UI() {
		this.setTitle("Carnet d'adresses");
		this.setSize(800,600);
		this.setLocationRelativeTo(null);
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setLayout(new BorderLayout());
		
		this.getContentPane().add(new JPanelCenter(), BorderLayout.CENTER);
	    this.getContentPane().add(new JPanelWest(), BorderLayout.WEST);
	    this.getContentPane().add(new JPanelEast(), BorderLayout.EAST);
	
		this.setVisible(true);
	}
}
